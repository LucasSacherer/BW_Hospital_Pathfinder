<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" prefWidth="300.0">
         <children>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <JFXButton buttonType="RAISED" onAction="#setDirectoryOrigin" text="Set Origin" />
                  <JFXButton buttonType="RAISED" layoutX="10.0" layoutY="10.0" onAction="#setDirectoryDestination" text="Set Destination" />
               </children>
            </HBox>
            <JFXComboBox fx:id="browser" promptText="Directory Browser" />
            <JFXListView fx:id="listView" layoutY="50" styleClass="custom-jfx-list-view">
            </JFXListView>
         </children>
      </VBox>
   </children>
</AnchorPane>
