<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane style="-fx-background-radius: 25;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="TOP_CENTER" style="-fx-background-radius: 25;">
         <children>
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <JFXButton buttonType="RAISED" onAction="#setDirectoryOrigin" text="Set Origin" />
                  <JFXButton buttonType="RAISED" layoutX="10.0" layoutY="10.0" onAction="#setDirectoryDestination" text="Set Destination" />
               </children>
            </HBox>
            <JFXComboBox fx:id="browser" promptText="Directory Browser" />
            <AnchorPane style="-fx-border-radius: 25;">
               <children>
                  <JFXListView fx:id="listView" prefHeight="400.0" prefWidth="280.0" styleClass="custom-jfx-list-view" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            </JFXListView>
               </children>
            </AnchorPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
